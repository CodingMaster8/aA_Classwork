<h2>All the bands!</h2>

<ul class='main-list'>
  <%= @bands.each do |band| %>
    <li><a href="<%= band_url(band) %>"><p><%= band.name %></p></a></li>
  <% end %>
</ul>

<h4 class='sub-header'>Links</h4>
<ul class='page-links'>
  <a class='button' href="<%= new_band_url %>">Create a new band!</a>
</ul>